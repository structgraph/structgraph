}

relationship.invokes {
  color: #68BDF6;
  border-color: #5CA8DB;
  text-color-internal: #FFFFFF;
}

relationship.hasMethod {
  color: #6DCE9E;
  border-color: #60B58B;
  text-color-internal: #FFFFFF;
